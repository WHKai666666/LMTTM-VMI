{
    "batch_size":96,
    "dataset_name":"organmnist3d",
    "root":"./datasets_data/medmnist",
    "model":{
        "drop_r":0.2,
        "process_unit_mode":"mixer",
        "other_process_unit_mode":"mlp,transformer",
        "summarize_mode":"TL",
        "other_summarize_mode":"TL-AddErase,TL-MHA",
        "in_channels":1,
        "dim":32,
        "memory_tokens_size":96,
        "num_tokens":8,
        "step":28,
        "out_class_num":11,
        "patch_size":3,
        "use_positional_embedding":"False"
    },
    "train":{
        "name":"t9_organmnist3d_TL_mixer_memory96_tokens8",
        "epoch":500,
        "optimizer":"Adam",
        "lr":1e-3,
        "weight_decay":1e-3,
        "load_memory_tokens":"False",
        "val_gap":100
    }
}