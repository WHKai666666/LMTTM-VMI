{
    "batch_size":100,
    "dataset_name":"organmnist3d",
    "root":"./data/medmnist",
    "train":{
        "name":"lapalce_noise_ttm",
        "epoch":250,
        "optimizer":"Adam",
        "lr":1e-3,
        "weight_decay":1e-4,
        "load_memory_tokens":"False",
        "save_check_point_only_once":"True",
        "val_gap":50
    },
    "model":{
        "drop_r":0.2,
        "process_unit_mode":"mixer",
        "other_process_unit_mode":"mlp,transformer",
        "summarize_mode":"TL",
        "other_summarize_mode":"TL-AddErase,TL-MHA",
        "in_channels":1,
        "dim":16,
        "memory_tokens_size":128,
        "num_tokens":8,
        "step":28,
        "out_class_num":11,
        "patch_size":4,
        "use_positional_embedding":"True"
    }
}