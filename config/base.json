{
    "batch_size": 64,
    "dataset_name": "organmnist3d",
    "all_dataset_name": "organmnist3d, hmdb_dataset0/1/2",
    "root": "./datasets_data/medmnist",
    "all_root": "./datasets_data/medmnist  ./datasets_data/hmdb/dataset0 1 2",
    "log_dir": "./logs",
    "checkpoint_dir": "./checkpoints",
    "model": {
        "model": "ttm",
        "_all_model_": "ttm,lmttm,lmttmV2",
        "drop_r": 0.2,
        "preprocess_mode": "resnet18",
        "all_preprocess_mode": "3d, resnet18",
        "process_unit": "transformer",
        "all_process_unit": "transformer, mixer,transformer",
        "memory_mode": "TL",
        "all_memory_mode": "TL-MHA, TL-AddErase, TL",
        "in_channels": 1,
        "dim": 96,
        "memory_tokens_size": 96,
        "num_blocks": 16,
        "summerize_num_tokens": 8,
        "step": 28,
        "all_steps": "18, 28",
        "out_class_num": 11,
        "patch_size": 3,
        "Read_use_positional_embedding": false,
        "Write_use_positional_embedding": true,
        "load_memory_add_noise": true,
        "load_memory_add_noise_mode": "uniform",
        "all_load_memory_add_noise_mode": "None, uniform, laplace, normal, exp, gamma, poisson",
        "variant": "variant0"
    },
    "train": {
        "name": "name_mednist",
        "epoch": 100,
        "optimizer": "Adam",
        "lr": 0.0001,
        "weight_decay": 0.0001,
        "load_memory_tokens": true,
        "input_H": 28,
        "input_W": 28,
        "val_gap": 100
    }
}