{
    "batch_size": 48,
    "dataset_name": "organmnist3d",
    "root": "./datasets_data/medmnist",
    "model": {
        "drop_r": 0.2,
        "process_unit": "transformer",
        "all_process_unit": "transformer, mixer,transformer",
        "memory_mode": "TL-MHA",
        "all_memory_mode": "TL-MHA, TL-AddErase, TL",
        "in_channels": 1,
        "dim": 128,
        "memory_tokens_size": 8192,
        "num_tokens": 64,
        "step": 28,
        "out_class_num": 11,
        "patch_size": 3,
        "Read_use_positional_embedding": false,
        "Write_use_positional_embedding": true,
        "load_memory_add_noise": true,
        "load_memory_add_noise_mode": "laplace",
        "all_load_memory_add_noise_mode": "uniform, laplace, normal, exp, gamma, poisson"
    },
    "train": {
        "name": "TTM_MHA_transformer_WritePosition_Load_laplace_mem8192_tokens64",
        "epoch": 1200,
        "optimizer": "Adam",
        "lr": 0.001,
        "weight_decay": 0.001,
        "load_memory_tokens": true,
        "val_gap": 100
    }
}