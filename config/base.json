{
    "batch_size": 64,
    "dataset_name": "nodulemnist3d",
    "all_dataset_name": "organmnist3d, nodulemnist3d, fracturemnist3d, adrenalmnist3d, vesselmnist3d, synapsemnist3d, hmdb_dataset0/1/2",
    "root": "./datasets_data/medmnist",
    "all_root": "./datasets_data/medmnist  ./datasets_data/hmdb/dataset0 1 2",
    "log_dir": "./logs",
    "checkpoint_dir": "./checkpoints",
    "model": {
        "model": "lmttm",
        "_all_model_": "ttm, lmttm",
